# Sample environment variables for the configuration service
# Copy this file to .env and modify as needed
# These can be overridden when running the application or Docker container

# Port on which the server will listen
PORT=3000

# Environment mode (e.g., development, production)
NODE_ENV=development

# Application name
APP_NAME=Config Service

# Example database URL (not used in this service, but can be extended)
DATABASE_URL=postgresql://user:password@localhost:5432/db
